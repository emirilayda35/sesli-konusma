{
    "productName": "sesli_konusma",
    "version": "0.1.0",
    "identifier": "com.seslikonusma.app",
    "build": {
        "beforeDevCommand": "npm run dev",
        "beforeBuildCommand": "npm run build",
        "devUrl": "http://localhost:5173",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "label": "main",
                "title": "Sesli Konu≈üma",
                "width": 1200,
                "height": 800,
                "resizable": true,
                "fullscreen": false,
                "visible": true,
                "center": true,
                "decorations": true,
                "transparent": false
            }
        ],
        "security": {
            "csp": null
        }
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    }
}